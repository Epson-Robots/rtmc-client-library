<?xml version="1.0" ?>
<dds>
 <permissions>
  <grant name="rc_permissions">
   <subject_name>emailAddress=User@example.com,CN=User_Example,OU=User_Example,O=User_Example,L=Azumino,ST=Nagano,C=JP</subject_name>
   <validity>
    <not_before>2025-01-01T00:00:00</not_before>
    <not_after>2030-01-01T00:00:00</not_after>
   </validity>
   <allow_rule>
    <domains>
     <id_range>
      <min>0</min>
      <max>230</max>
     </id_range>
    </domains>
    <publish>
     <topics>
      <topic>DDSCommunicationTopic</topic>
     </topics>
     <partitions>
      <partition>RTMC_RES</partition>
     </partitions>
    </publish>
    <subscribe>
     <topics>
      <topic>DDSCommunicationTopic</topic>
     </topics>
     <partitions>
      <partition>RTMC_CMD</partition>
     </partitions>
    </subscribe>
   </allow_rule>
   <default>DENY</default>
  </grant>
 </permissions>
</dds>
